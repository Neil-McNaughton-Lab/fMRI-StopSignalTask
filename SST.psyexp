        <Param name="Begin Routine" updates="constant" val="#Compute wait time after subject response:&amp;#10;if not isinstance(gokey_resp.rt, float):&amp;#10;    gokey_resp.rt = 1.0&amp;#10;timeremaining = 1.0 - gokey_resp.rt&amp;#10;&amp;#10;## Determine feedback:&amp;#10;# Check if the subject responded slower than allowed:&amp;#10;if (gokey_resp.rt &gt; goMax):# and (timeremaining &gt; 0.0):&amp;#10;    img_path = 'display_images/slow.JPG'&amp;#10;else:&amp;#10;    # Otherwise check if they responded correctly:&amp;#10;    if (gokey_resp.keys == LEFT) and (arrow == '&lt;='):&amp;#10;        img_path = 'display_images/Smiley_Black2.JPG'&amp;#10;    elif (gokey_resp.keys == RIGHT) and (arrow == '=&gt;'):&amp;#10;        img_path = 'display_images/Smiley_Black2.JPG'&amp;#10;    else:&amp;#10;        img_path = 'display_images/FrowneyBlue_Black2.JPG'&amp;#10;&amp;#10;# Store reaction time for SSD computation:&amp;#10;if gokey_resp.rt is not []:     # &lt;- Filter out no responces&amp;#10;    goRTs[rt_counter] = gokey_resp.rt&amp;#10;    rt_counter = (rt_counter + 1)%nRTaves" valType="extendedCode"/>
        <Param name="Code Type" updates="None" val="Py" valType="str"/>
