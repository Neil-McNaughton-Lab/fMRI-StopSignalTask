    <Routine name="SETTINGS">
      <CodeComponent name="user_settings">
        <Param name="Before Experiment" updates="constant" val="&quot;&quot;&quot;##########################################################################&quot;&quot;&quot;&amp;#10;&quot;&quot;&quot;################                                          ################&quot;&quot;&quot;&amp;#10;&quot;&quot;&quot;####                             SETTINGS                             ####&quot;&quot;&quot;&amp;#10;&quot;&quot;&quot;################                                          ################&quot;&quot;&quot;&amp;#10;&quot;&quot;&quot;##########################################################################&quot;&quot;&quot;&amp;#10;# Conditions path:&amp;#10;conditions_path = &quot;conditions.xlsx&quot;&amp;#10;&amp;#10;# This block is for defining settings that are hard &amp;#10;# to call from psychopy because of it's order the builder's execution.&amp;#10;&amp;#10;# Default sound values. &amp;#10;# (Over written by those chosen using the python script called within the 'ext_scripts' code block)&amp;#10;sound_volume = 0.06 # value between 0 and 1&amp;#10;sound_frequency = 500 # Hz&amp;#10;&amp;#10;# Inter-Stimulus Interval settings (see 'Instructions' -&gt; &quot;run_on_start&quot;):&amp;#10;meanITItime = 1000    # ms&amp;#10;ITImin      = 500     # ms&amp;#10;ITImax      = 4000    # ms&amp;#10;ITIfactor   = 125     # ms&amp;#10;&amp;#10;# Maximum time the subjects have to respond (see 'Rest_Period' -&gt; &quot;block_prep&quot;):&amp;#10;GoMaxMultiplier = 1.0 #Sets the max time they have to their meanRT*GoMaxMultiplier&amp;#10;&amp;#10;# SSD settings (see 'Rest_Period' -&gt; &quot;block_prep&quot;):&amp;#10;startSSD1 = 0.80 # The fraction used to compute SSD1 from the subject's go-RT (between 0 &amp; 1)&amp;#10;startSSD2 = 0.45 # The fraction used to compute SSD2 from the subject's go-RT (between 0 &amp; 1)&amp;#10;startSSD3 = 0.20 # The fraction used to compute SSD3 from the subject's go-RT (between 0 &amp; 1)&amp;#10;stepSize = 0.050 # Amount an SSD will increase/decrease by in each stop trial in ms&amp;#10;minSSD = 0.005   # ms&amp;#10;maxSSD = 1.0     # ms&amp;#10;&amp;#10;nRTaves = 16 # The number of most recent go trials to average over when computing each SSD&amp;#10;&amp;#10;&amp;#10;# Stimulus and feedback images:&amp;#10;fb_size = 0.6675       #Adjusts &quot;'size [w,h]'&quot; in &quot;feedback_img&quot; in 'go_feedback'/'stop_feedback'&amp;#10;arrowStimSize = 0.18   #Adjusts &quot;'letter hight'&quot; in &quot;arrow_stim_go&quot; in 'go_stim'/'stop_stim'&amp;#10;circle_size = 1.24     #Adjusts &quot;'size [w,h]'&quot; in &quot;green_go_circle&quot; &amp; &quot;focus_circle&quot; in 'go_stim'/'stop_stim' &amp; 'standby' respectively&amp;#10;circle_thickness = 3   #Adjusts &quot;'Line width'&quot; in &quot;green_go_circle&quot; &amp; &quot;focus_circle&quot; in 'go_stim'/'stop_stim' &amp; 'standby' respectively" valType="extendedCode"/>
        <Param name="Before JS Experiment" updates="constant" val="/*  Load the conditions file early because psychopy doesn't load at startup  */&amp;#10;import * as pandas from 'pandas';&amp;#10;var Ntrials, Ntrials2, current_trial, df;&amp;#10;df = pandas.read_excel(&quot;conditions.xlsx&quot;);&amp;#10;Ntrials = df.length;&amp;#10;/*  Make a trial counter  */&amp;#10;current_trial = 1;&amp;#10;/*  Generate ISI times by sampling a -np.log(x)/lamb logarithmic distribution  */&amp;#10;Ntrials2 = 2;&amp;#10;" valType="extendedCode"/>
        <Param name="Begin Experiment" updates="constant" val="# Generate circle stim diameter from screen resolution&amp;#10;diameter = (circle_size)*296/win.size[0] #win.size gives the resolution you set&amp;#10;&amp;#10;# This is the resolution of feedback images:&amp;#10;feedback_image_res = [960, 720]&amp;#10;img_ratio = feedback_image_res[0]/feedback_image_res[1]   #4/3&amp;#10;#win.ratio = win.size[0]/win.size[1]&amp;#10;print(expInfo, win.size, img_ratio)&amp;#10;print('^^^^^^^^^^^^^^^^^^^^^^^^')&amp;#10;import tkinter" valType="extendedCode"/>
        <Param name="Begin JS Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="Begin JS Routine" updates="constant" val="" valType="extendedCode"/>
        <Param name="Begin Routine" updates="constant" val="" valType="extendedCode"/>
        <Param name="Code Type" updates="None" val="Py" valType="str"/>
        <Param name="Each Frame" updates="constant" val="" valType="extendedCode"/>
        <Param name="Each JS Frame" updates="constant" val="" valType="extendedCode"/>
        <Param name="End Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="End JS Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="End JS Routine" updates="constant" val="" valType="extendedCode"/>
