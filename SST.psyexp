      <CodeComponent name="run_on_start">
        <Param name="Before Experiment" updates="constant" val="import pandas&amp;#10;import numpy as np&amp;#10;#print(pandas.__version__)&amp;#10;&amp;#10;################################################################################&amp;#10;###########################   FUNCTIONS   ######################################&amp;#10;################################################################################&amp;#10;def checkConditionsFile(df):&amp;#10;    ''' This function checks if there is something wrong with the user's &amp;#10;        conditions file. If there is, it returns an error explaining what's &amp;#10;        wrong.&amp;#10;        &amp;#10;        Parameters&amp;#10;        ----------&amp;#10;        df : panadas.dataframe&amp;#10;            A pandas data frame that conditions.xlsx has been loaded into via&amp;#10;            the pandas method: pandas.read_excel()&amp;#10;    '''&amp;#10;    def _build_index_str(str_list, column_name):&amp;#10;        # Builds the appropriate string from a list of indcies.&amp;#10;        if len(str_list) == 1:&amp;#10;            str_list = str_list[0]&amp;#10;            string = ('', str_list, column_name, 's an', '')&amp;#10;        elif len(str_list) == 2:&amp;#10;            str_list = str_list[0] + ' and ' + str_list[1]&amp;#10;            string = ('s', str_list, column_name, '', 's')&amp;#10;        else:&amp;#10;            str_list[-1] = 'and ' + str_list[-1]&amp;#10;            str_list = ', '.join(str_list)&amp;#10;            string = ('s', str_list, column_name, '', 's')&amp;#10;        &amp;#10;        string = 'Row%s %s in the %s column of your conditions file contain%s empty field%s!' % string &amp;#10;        return string&amp;#10;        &amp;#10;    # Check no columns are missing in the conditions file:&amp;#10;    headers = df.columns.tolist()&amp;#10;    if 'TrialType' not in headers:&amp;#10;        raise KeyError('Your conditions file must contain a TrialType column '&amp;#10;                       'indicating which trials are Go trials (0), or Stop'&amp;#10;                       'trials (1, 2, or 3).')&amp;#10;        &amp;#10;    if 'Block' not in headers:&amp;#10;        raise KeyError('Your conditions file must contain a Block column '&amp;#10;                       'indicating the block number each trial belongs to.')&amp;#10;&amp;#10;    if 'BlockType' not in headers:&amp;#10;        raise KeyError('You must include a BlockType column in your '&amp;#10;                       'conditions file!\nThe column must have a &quot;practice&quot; '&amp;#10;                       'or real trial (e.g. &quot;fMRI&quot;) identifier on each row '&amp;#10;                       'that a new block begins.')&amp;#10;&amp;#10;    # Check TrialType and Block are not empty:&amp;#10;    if any(df[&quot;TrialType&quot;].isnull()):&amp;#10;        index = [str(i+2) for i, tf in enumerate(df[&quot;TrialType&quot;].isnull()) if tf is True]&amp;#10;        raise ValueError(_build_index_str(index, 'TrialType'))&amp;#10;    &amp;#10;    if any(df[&quot;Block&quot;].isnull()):&amp;#10;        index = [str(i+2) for i, tf in enumerate(df[&quot;Block&quot;].isnull()) if tf is True]&amp;#10;        raise ValueError(_build_index_str(index, 'Block'))&amp;#10;        &amp;#10;    # Check the Block numbers make sense:&amp;#10;    switch = df['Block'].diff().tolist()[1:] + [1.0]&amp;#10;    if any([True for i in switch if ((i != 1.0) and (i != 0.0))]):&amp;#10;        raise ValueError('The Block column in your conditions file doesn\'t '&amp;#10;                         'have linearly increasing block numbers!')&amp;#10;    &amp;#10;    # Check BlockType has an input on each row that a new trial begins:&amp;#10;    switch_indecies = [i for i, val in enumerate(switch) if val == 1.0]&amp;#10;    nan_present = [True for i in switch_indecies if str(df['BlockType'].iloc[i]) == 'nan']&amp;#10;    if any(nan_present):&amp;#10;        raise ValueError('The BlockType column in your conditions file must '&amp;#10;                         'have a &quot;practice&quot; or real trial (e.g. &quot;fMRI&quot;) '&amp;#10;                         'identifier on each row that a new block begins.')&amp;#10;&amp;#10;&amp;#10;def gen_LR_array(count, LRratio=-1, disp=False, symb=['&lt;=','=&gt;']):&amp;#10;    ''' This function creates a random array of right and lefts with &amp;#10;        with the ration of lefts to rights being defined by the LRratio input. &amp;#10;        &amp;#10;        count : int&amp;#10;            Length of the desired left/right array.&amp;#10;            &amp;#10;        LRratio : float, default=-1&amp;#10;            Ratio of '&lt;='s to '=&gt;'s in the array. If LRratio is not within &amp;#10;            the range [0, 1] then the ratio of lefts to rights will be random.&amp;#10;            &amp;#10;        disp : bool, default=False&amp;#10;            Prints the number of lefts and rights present in the output array&amp;#10;        &amp;#10;        symb : list, default=['&lt;=','=&gt;']&amp;#10;            What symbol to use in the list for left and right respectievly.&amp;#10;        &amp;#10;        Outputs&amp;#10;        -------&amp;#10;        direction : list of str&amp;#10;            Left-right list to print during the sequence&amp;#10;    '''&amp;#10;    #Check LRratio and count:&amp;#10;    if not isinstance(count, int):&amp;#10;        raise ValueError('the input \'count\' must be an int!')&amp;#10;    &amp;#10;    if not (isinstance(LRratio, float) or isinstance(LRratio, int)):&amp;#10;        raise ValueError('the input \'LRratio\' must be an int or a float!')&amp;#10;        &amp;#10;    if len(symb) != 2:&amp;#10;        raise ValueError('The input, symb, must be a list of length 2. '&amp;#10;                         'The element in index 0 corresponding to left, '&amp;#10;                         'and the elment in index 1 corresponding to right.')&amp;#10;    &amp;#10;    #Build a random array of rights and lefts:&amp;#10;    direction = []&amp;#10;    left_index = []&amp;#10;    right_index = []&amp;#10;    for index, ran in enumerate(np.random.rand(count)):&amp;#10;        if ran &lt;= 0.5:&amp;#10;            direction.append(symb[0])&amp;#10;            left_index.append(index)&amp;#10;        else:&amp;#10;            direction.append(symb[1])&amp;#10;            right_index.append(index)&amp;#10;    &amp;#10;    if (LRratio &gt;= 0) and (LRratio &lt;= 1):&amp;#10;        # Correct the distribution of lefts and rights:&amp;#10;        excess = len(left_index) - round(count*LRratio) &amp;#10;        if excess &gt; 0: #There are more lefts than rights&amp;#10;            # Sample the left array and change direction appopriately:&amp;#10;            for ex in range(excess):&amp;#10;                target = int(np.floor(np.random.rand(1)*len(left_index)))&amp;#10;                direction[left_index[target]] = symb[1]&amp;#10;                left_index.pop(target)&amp;#10;        &amp;#10;        elif excess &lt; 0: #There are more rights than lefts&amp;#10;            # Sample the right array and change direction appopriately:&amp;#10;            for ex in range(-excess):&amp;#10;                target = int(np.floor(np.random.rand(1)*len(right_index)))&amp;#10;                direction[right_index[target]] = symb[0]&amp;#10;                right_index.pop(target)&amp;#10;&amp;#10;    if disp:&amp;#10;        print(' Lefts: ', len([1 for i in direction if i == symb[0]]), ', ', &amp;#10;              'Rights: ', len([1 for i in direction if i == symb[1]]))&amp;#10;&amp;#10;    return direction&amp;#10;&amp;#10;def getLRratios(df_column):&amp;#10;    ''' Cpnverts either the &quot;L2R_ratio&quot; or &quot;BlockType&quot; arrays in your &amp;#10;        conditions.xlsx file to a list of ratios which can be used to&amp;#10;        assign the &lt;= =&gt; stimuli for each trial. If no &quot;L2R_ratio&quot; is &amp;#10;        present &quot;BlockType&quot; is used by defaulting 'practice' trials to&amp;#10;        a 50:50 ratio of &lt;= and =&gt; stimuli and everything else to&amp;#10;        a random ratio. See gen_LR_array for valid ratios.&amp;#10;    '''&amp;#10;    ratios = []&amp;#10;    for i, r in enumerate(df_column):&amp;#10;        if str(r) == 'nan':&amp;#10;            ratios.append(r)&amp;#10;        elif not isinstance(r, str):&amp;#10;            ratios.append(r)&amp;#10;        elif r.lower() == 'practice':&amp;#10;            ratios.append(0.5)&amp;#10;        else:&amp;#10;            ratios.append(-1) #random&amp;#10;    return ratios&amp;#10;################################################################################&amp;#10;################################################################################&amp;#10;&amp;#10;## Load the conditions file early because psychopy doesn't load on startup&amp;#10;df = pandas.read_excel(conditions_path)&amp;#10;&amp;#10;&amp;#10;## Make trial, block, and SSDave loop counters&amp;#10;current_trial = 0&amp;#10;current_block = 0&amp;#10;current_block_trial = 0&amp;#10;rt_counter = 0&amp;#10;&amp;#10;&amp;#10;## Generate necessary variables for temporarily storing the SSD staircases:&amp;#10;SSD = [0.0, 0.0, 0.0] #placeholder until actual values are generated by the subject&amp;#10;goRTs = [None]*nRTaves&amp;#10;&amp;#10;&amp;#10;## Inital maximum time subjects have to respond:&amp;#10;# (will be overwritten after the first set of Go trials)&amp;#10;goMax = 1.0 #s&amp;#10;&amp;#10;&amp;#10;## Generate Stim Arrows: &amp;#10;# Generate the arrow directions which will presented to the participant:&amp;#10;#1) Get Left to Right ratios listed by the &amp;#10;#   user or create defaults if none are supplied:&amp;#10;if 'L2R_ratio' in df.columns.tolist():&amp;#10;    if all(df[&quot;L2R_ratio&quot;].isnull()):&amp;#10;        ratios = getLRratios(df['BlockType']) #If the column's there but empty&amp;#10;    else:&amp;#10;        ratios = getLRratios(df['L2R_ratio']) #If the column's not empty&amp;#10;else:&amp;#10;    ratios = getLRratios(df['BlockType']) #If the column's not there&amp;#10;&amp;#10;&amp;#10;# Get when different blocks occur:&amp;#10;switch = df['Block'].diff().tolist()[1:] + [1.0]&amp;#10;&amp;#10;&amp;#10;# create the direction list:&amp;#10;direction = []&amp;#10;count = 1&amp;#10;for i in range(len(df)):&amp;#10;    # Check L2R_ratio for nans:&amp;#10;    if str(ratios[i]) != 'nan':&amp;#10;        LRratio = ratios[i]&amp;#10;        if isinstance(LRratio, str):&amp;#10;            LRratio = -1&amp;#10;    &amp;#10;    #Create the block's array:&amp;#10;    if switch[i] == 0.0:&amp;#10;        count += 1&amp;#10;    else:&amp;#10;        direction = direction + gen_LR_array(count, LRratio)&amp;#10;        count = 1&amp;#10;&amp;#10;&amp;#10;&amp;#10;## Generating ISI times before the task to save on mid-task processing time:&amp;#10;ITInulltime = [1375, 625, 750, 2625, 3000, 1000, 500, 4000, 1000, 625, &amp;#10;               1625, 2250, 1250, 2375, 1625, 875, 3000, 3250, 625, 2875, &amp;#10;               625, 1250, 2875, 1375, 1625, 3125, 625, 500, 2000, 500, &amp;#10;               500, 875, 2750, 750, 625, 2125, 1125, 1125, 1250, 1000, &amp;#10;               2500, 2500, 1500, 1250, 1000, 1750, 625, 1875, 1625, 500, &amp;#10;               1875, 1375, 625, 2250, 1000, 2125, 875, 1125, 1125, 3750, &amp;#10;               2250, 500, 2125, 1125, 1500, 625, 1000, 500, 1750, 750, &amp;#10;               1375, 1875, 875, 3500, 750, 500, 2000, 1250, 875, 1375, &amp;#10;               1500, 625, 625, 1250, 2125, 750, 625, 750, 875, 1750, &amp;#10;               1750, 2375, 2500, 750, 750, 625, 1875, 750, 2125, 750, &amp;#10;               3375, 2625, 1625, 1000, 1875, 750, 500, 3375, 500, 625, &amp;#10;               3875, 500, 1750, 2000, 500, 2750, 625, 625, 1750, 875, &amp;#10;               1000, 875, 1125, 1500, 1125, 1375, 750, 1500, 1375, 750, &amp;#10;               625, 2375, 2000, 625, 2625, 1500, 1000]&amp;#10;# # I need to sample the above array for the null time. The appraoch I will take&amp;#10;# # is I will use the block array from the conditions file to determine when a &amp;#10;# # new block occurs. Then I will find the indexes of these new blocks. Finally,&amp;#10;# # I will use index1 to index2 to choose the correct sample size to take from &amp;#10;# # the  above array.&amp;#10;# index = df['Block'].diff()&amp;#10;# index.iloc[0] = 1.0&amp;#10;# index = [i for i, val in enumerate(index) if val == 1.0] + [len(index)]&amp;#10;# ITIChoices = []&amp;#10;# for i in range(len(index)-1):&amp;#10;#     ITIChoices = ITIChoices + ITInulltime[index[i]:index[i+1]]&amp;#10;# ITIChoices = [isi/1000 for isi in ITIChoices] # The 1/1000 converts to seconds&amp;#10;ITIChoices = [i/1000 for i in ITInulltime]&amp;#10;&amp;#10;## Identify which blocks are go_only blocks:&amp;#10;# This is needed in order to determine which messages to display to the subject&amp;#10;# in 'Rest_Period' -&gt; 'block_prep'&amp;#10;stops_started_ref = False&amp;#10;trial_types = [bool(tt) for i, tt in enumerate(df['TrialType'])]&amp;#10;block_n = [b for i, b in enumerate(df['Block'])]&amp;#10;&amp;#10;stops_start = block_n[-1]+1&amp;#10;for i, tt in enumerate(trial_types):&amp;#10;    if tt:&amp;#10;        stops_start = block_n[i]&amp;#10;        break&amp;#10;" valType="extendedCode"/>
        <Param name="Before JS Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="Begin Experiment" updates="constant" val="# Check current subject for left/right handedness&amp;#10;print(sub_data['handedness'])&amp;#10;if sub_data['handedness'] == 'Left':&amp;#10;    LEFT = '8'&amp;#10;    RIGHT = '7'&amp;#10;elif sub_data['handedness'] == 'Right':&amp;#10;    LEFT = '2'&amp;#10;    RIGHT = '3'&amp;#10;else:&amp;#10;     error_text = 'Invlaid handedness!'&amp;#10;&amp;#10;# Store the keys that the subject can press during stim:&amp;#10;possible_keys = (LEFT, RIGHT)" valType="extendedCode"/>
        <Param name="Begin JS Experiment" updates="constant" val="/*  Check current subject for left/right handedness  */&amp;#10;var LEFT, RIGHT, error_text, handedness;&amp;#10;handedness = expInfo[&quot;handedness&quot;].toString().lower().strip();&amp;#10;if ((handedness === &quot;left&quot;)) {&amp;#10;    LEFT = 8;&amp;#10;    RIGHT = 7;&amp;#10;} else {&amp;#10;    if ((handedness === &quot;right&quot;)) {&amp;#10;        LEFT = 2;&amp;#10;        RIGHT = 3;&amp;#10;    } else {&amp;#10;        error_text = &quot;Invlaid handedness!&quot;;&amp;#10;    }&amp;#10;}&amp;#10;" valType="extendedCode"/>
