        <Param name="disabled" updates="None" val="False" valType="bool"/>
        <Param name="name" updates="None" val="user_settings" valType="code"/>
      </CodeComponent>
      <CodeComponent name="ext_scripts">
        <Param name="Before Experiment" updates="constant" val="## Subject information:&amp;#10;from external_functions import participant_details_GUI_v2p2 as pdg&amp;#10;sub_data = pdg.pardetGUI()&amp;#10;# Check if the GUI was exited prematurely and end the experiment:&amp;#10;if sub_data['soundtest'] == 100:&amp;#10;    quit()&amp;#10;&amp;#10;&amp;#10;" valType="extendedCode"/>
        <Param name="Before JS Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="Begin Experiment" updates="constant" val="## Handle subject information:&amp;#10;# Pass subject data to PsychoPy:&amp;#10;expInfo['participant'] = sub_data['participant']&amp;#10;expInfo['gender'] = 'N/A' #sub_data['gender']&amp;#10;expInfo['handedness'] = sub_data['handedness']&amp;#10;expInfo['ethnicity'] = sub_data['ethnicities']&amp;#10;run_sound_test = sub_data['soundtest']&amp;#10;" valType="extendedCode"/>
        <Param name="Begin JS Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="Begin JS Routine" updates="constant" val="" valType="extendedCode"/>
        <Param name="Begin Routine" updates="constant" val="## This block is used to point at external psychopy scripts I've written to &amp;#10;## record subject information prior to the experiment. Ideally this will make &amp;#10;## it easier for people to use them in their own tasks without having to &amp;#10;## directly edit them into psychopy. The worse case scenario is that eveything &amp;#10;## breaks because of version differneces.&amp;#10;&amp;#10;## Volume Controls:&amp;#10;from external_functions import test_soundv1p1 as ts&amp;#10;if run_sound_test == 1:&amp;#10;    sound_volume, sound_frequency = \&amp;#10;            ts.TestSound(win, defaultKeyboard, expInfo,  nReps=100,&amp;#10;                         frameTolerance=frameTolerance, endExpNow=endExpNow)&amp;#10;&amp;#10;" valType="extendedCode"/>
        <Param name="Code Type" updates="None" val="Py" valType="str"/>
        <Param name="Each Frame" updates="constant" val="" valType="extendedCode"/>
        <Param name="Each JS Frame" updates="constant" val="" valType="extendedCode"/>
        <Param name="End Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="End JS Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="End JS Routine" updates="constant" val="" valType="extendedCode"/>
        <Param name="End Routine" updates="constant" val="" valType="extendedCode"/>
        <Param name="disabled" updates="None" val="False" valType="bool"/>
        <Param name="name" updates="None" val="ext_scripts" valType="code"/>
      </CodeComponent>
      <CodeComponent name="resave_data">
        <Param name="Before Experiment" updates="constant" val="import SST_cleandata_v1p1&amp;#10;&amp;#10;# Monkey-Patch stop function (bad practice, yes, but currently the best I can do):&amp;#10;def quit():&amp;#10;    &quot;&quot;&quot;Close everything and exit nicely (ending the experiment)&amp;#10;    &quot;&quot;&quot;&amp;#10;    # pygame.quit()  # safe even if pygame was never initialised&amp;#10;    logging.flush()&amp;#10;&amp;#10;    for thisThread in core.threading.enumerate():&amp;#10;        if hasattr(thisThread, 'stop') and hasattr(thisThread, 'running'):&amp;#10;            # this is one of our event threads - kill it and wait for success&amp;#10;            thisThread.stop()&amp;#10;            while thisThread.running == 0:&amp;#10;                pass  # wait until it has properly finished polling&amp;#10;    try:&amp;#10;        # Pre-save the data so that I can call my re-write function:&amp;#10;        # (This pre-call is do to avoid monkey-patching the saveAsWideText method)&amp;#10;        thisExp.saveAsWideText(thisExp.dataFileName + '.csv', delim=',')&amp;#10;        &amp;#10;        print(&quot;Executing Rhys' monkey-patched stop function!!&quot;)&amp;#10;        cleandata.resave(thisExp.dataFileName)&amp;#10;    except:# NameError:&amp;#10;        # If the data-file was never created then we just exit normally:&amp;#10;        pass&amp;#10;    sys.exit(0)  # quits the python session entirely&amp;#10;&amp;#10;core.quit = quit # Monkey-Patch psychopy's quit method&amp;#10;" valType="extendedCode"/>
        <Param name="Before JS Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="Begin Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="Begin JS Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="Begin JS Routine" updates="constant" val="" valType="extendedCode"/>
        <Param name="Begin Routine" updates="constant" val="" valType="extendedCode"/>
        <Param name="Code Type" updates="None" val="Py" valType="str"/>
        <Param name="Each Frame" updates="constant" val="" valType="extendedCode"/>
        <Param name="Each JS Frame" updates="constant" val="" valType="extendedCode"/>
        <Param name="End Experiment" updates="constant" val="#file_path = thisExp.dataFileName&amp;#10;&amp;#10;" valType="extendedCode"/>
        <Param name="End JS Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="End JS Routine" updates="constant" val="" valType="extendedCode"/>
        <Param name="End Routine" updates="constant" val="" valType="extendedCode"/>
        <Param name="disabled" updates="None" val="True" valType="bool"/>
        <Param name="name" updates="None" val="resave_data" valType="code"/>
