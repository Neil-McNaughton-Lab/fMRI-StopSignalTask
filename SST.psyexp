        <Param name="Begin JS Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="Begin JS Routine" updates="constant" val="" valType="extendedCode"/>
        <Param name="Begin Routine" updates="constant" val="## Average the subject's last 16 RT's:&amp;#10;aveRT = np.mean(list(filter(None, goRTs)))&amp;#10;&amp;#10;## Calculate starting SSDs:&amp;#10;#       (SSD values are adjusted within 'stop_feedback' -&gt; &quot;checkKeypress_stop&quot;)&amp;#10;#       (SSD values are saved within 'ITI' -&gt; &quot;writeSSD&quot;)&amp;#10;SSD = [startSSD1*aveRT, startSSD2*aveRT, startSSD3*aveRT]&amp;#10;# Incase the subject doesn't push anything ever:&amp;#10;if np.isnan(SSD[0]) and np.isnan(SSD[1]) and np.isnan(SSD[2]):&amp;#10;    SSD = [0.25]*3&amp;#10;    &amp;#10;## Create a maximum amount of time the subject has to respond based on their &amp;#10;## usual response time:&amp;#10;goMax = aveRT*GoMaxMultiplier&amp;#10;thisExp.addData('goMax', goMax)&amp;#10;print('goMax response time: ', goMax)&amp;#10;print('SSD times: ', SSD)&amp;#10;&amp;#10;## Check the block type in the conditions file:&amp;#10;if BlockType.lower() == 'practice':&amp;#10;    init_real_trial = 0&amp;#10;else:&amp;#10;    init_real_trial = 1&amp;#10;&amp;#10;## Choose which message to display to the subject:&amp;#10;if init_real_trial == 0:&amp;#10;    DISP = DISP1&amp;#10;elif current_block == stops_start:&amp;#10;    DISP = DISP2&amp;#10;    stops_started_ref = True&amp;#10;elif stops_started_ref:&amp;#10;    DISP = DISP3&amp;#10;else:&amp;#10;    DISP = DISP1&amp;#10;    &amp;#10;# Reset counters:&amp;#10;current_block_trial = 0" valType="extendedCode"/>
        <Param name="Code Type" updates="None" val="Py" valType="str"/>
        <Param name="Each Frame" updates="constant" val="" valType="extendedCode"/>
        <Param name="Each JS Frame" updates="constant" val="" valType="extendedCode"/>
        <Param name="End Experiment" updates="constant" val="" valType="extendedCode"/>
        <Param name="End JS Experiment" updates="constant" val="" valType="extendedCode"/>
